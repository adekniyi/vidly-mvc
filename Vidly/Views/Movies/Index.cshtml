@model Vidly.ViewModel.RandomMovieViewModel
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@*<h2>@Model.Name</h2>*@
@foreach (var movie in Model.Movie)
{
<li class="mt-20">
   @* @movie.Name*@
    @Html.ActionLink(@movie.Name, "Details", "Movies", new
{
    id = @movie.Id
}, null);
</li>
}